{
 "compilerOptions": {
  "target": "esnext",
  "module": "esnext",
  "jsx": "react",
  "lib": ["es6"],
  "esModuleInterop": true,
  "allowSyntheticDefaultImports": true,
  "moduleResolution": "node",
  "resolveJsonModule": true,
  "allowJs": true,
  "noEmit": true,
  "strict": true,
  "baseUrl": "src"
 },
 "exclude": [
  "node_modules",
  "babel.config.js",
  "metro.config.js",
  "jest.config.js",
  "__tests__"
 ],
 "include": [
  "src", "index.tsx"
 ]
}